
@model TezYonetimSistemi.UI.Models.OgrenciTezPartialVievModel
@{
    Layout = null;
}
<h2>Tez Detay</h2>

<div class="form-horizontal">

    <div class="form-group">
        <div class="col-md-2">
            <label class="control-label">Tez Adı</label>
        </div>
        <label class="control-label tezAdi">@Model.TezAdi</label>
    </div>
    <div class="form-group">

        <div class="col-md-2">
            <label class="control-label">Ogretim Görevlisi</label>
        </div>
        <label class="control-label">@Model.Ogretmen</label>
    </div>
    <div class="form-group">
        <div class="col-md-2">
            <label class="control-label">Tez Ders Kodu</label>
        </div>
        <label class="control-label">@Model.TezDersKod</label>

    </div>
    <div class="form-group">
        <div class="col-md-2">
            <label class="control-label">Tez Dönemi</label>
        </div>
        <label class="control-label">@Model.TezDonemi</label>

    </div>
    <div class="form-group">
        <div class="col-md-2">
            <label class="control-label">Alan Öğrenciler</label>
        </div>
        @foreach (var ogr in Model.Ogrenciler)
        {
            <label class="control-label">
                @Html.ActionLink(ogr.Value, "Detay", "Ogrenci", new { id = ogr.Key }, null)
            </label>

        }
    </div>
</div>

@section styles{
    <link href="/assets/pages/css/profile.min.css" rel="stylesheet" type="text/css" />
}


@section scripts {
    <script src="/assets/global/plugins/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="/assets/pages/scripts/profile.min.js" type="text/javascript"></script>
}