@model IEnumerable<TezYonetimSistemi.UI.Models.OgrenciTezPartialVievModel>

<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-globe"></i>Öğrencinin Aldığı Tezler
                </div>
                <div class="tools"> </div>
            </div>
            <div class="portlet-body">
                <table class="table table-striped table-bordered table-hover" id="sample_2">
                    <thead>
                        <tr>
                            <th>
                                Tez Adı
                            </th>
                            <th>
                                Dönemi
                            </th>
                            <th>
                                Ders Kodu
                            </th>
                            <th>Ogretmeni</th>
                            <th>Alan Öğrenciler</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>

                                <td>
                                    @Html.DisplayFor(modelItem => item.TezAdi)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TezDonemi)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TezDersKod)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Ogretmen)
                                </td>
                                <td>
                                    @foreach (KeyValuePair<int, string> ogr in item.Ogrenciler)
                                    {
                                        @Html.ActionLink(ogr.Value, "Detay", "Ogrenci", new { id = ogr.Key }, null)<br />

                                    }

                                    @*@foreach (string ogr in item.Ogrenciler)
                                        {
                                            @Html.DisplayFor(modelItem => ogr)
                                            <br />
                                        }*@
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
